{
  "type": "module",
  "name": "visabuddy-mobile",
  "version": "1.0.0",
  "description": "VisaBuddy React Native Mobile App",
  "main": "index.js",
  "scripts": {
    "dev": "npx react-native start",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "test": "jest",
    "lint": "eslint .",
    "build:android": "cd android && ./gradlew assembleRelease && cd ..",
    "build:ios": "cd ios && xcodebuild -workspace VisaBuddy.xcworkspace -scheme VisaBuddy -configuration Release && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.15",
    "@react-navigation/stack": "^6.4.0",
    "axios": "^1.6.8",
    "babel-preset-expo": "^9.5.0",
    "date-fns": "^3.6.0",
    "expo": "^49.0.23",
    "expo-cli": "^6.3.12",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "^6.0.8",
    "i18next": "^25.5.3",
    "react": "^18.2.0",
    "react-hook-form": "^7.64.0",
    "react-i18next": "^15.3.4",
    "react-native": "^0.72.10",
    "react-native-gesture-handler": "^2.16.1",
    "react-native-reanimated": "^3.13.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.31.1",
    "react-native-vector-icons": "^10.1.0",
    "zod": "^3.25.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@eslint/js": "^9.0.0",
    "@react-native-community/cli": "^13.6.9",
    "@react-native-community/cli-platform-android": "^13.0.0",
    "@testing-library/react-native": "^12.4.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@types/react": "^18.2.0",
    "@types/react-native": "^0.73.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.34.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "typescript": "^5.9.0"
  }
}
